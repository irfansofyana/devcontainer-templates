{
    "id": "go-toolkit",
    "version": "1.0.2",
    "name": "Go Development Toolkit",
    "description": "This is a devcontainer template for some of the typical stacks used in Go development such as MySQL, Redis, Kafka, etc.",
    "documentationURL": "https://github.com/irfansofyana/devcontainer-templates/tree/main/src/go-toolkit",
    "publisher": "irfansofyana",
    "licenseURL": "https://github.com/irfansofyana/devcontainer-templates/blob/main/LICENSE",
    "options": {
        "imageVariant": {
            "type": "string",
            "description": "Go version to use:",
            "proposals": [
                "1.23",
                "1.22"
            ],
            "default": "1.23"
        }
    },
    "platforms": ["Go"],
    "optionalPaths": [
        ".github/*"
    ]
}